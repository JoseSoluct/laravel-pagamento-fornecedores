{
    "name": "rede-cauzzo-mais/pagamento",
    "description": "Pagamentos fornecedores CNAB 240 - Laravel",
    "keywords": [
        "CNAB240",
        "pagamento",
        "remessa",
        "retorno"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Almeida",
            "email": "brunob1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "knplabs/knp-snappy": "^1.5",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "RedeCauzzoMais\\Pagamento\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
